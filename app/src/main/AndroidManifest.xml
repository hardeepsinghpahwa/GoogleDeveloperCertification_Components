<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practicework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AndroidCore.Notification"></activity>
        <activity android:name=".AndroidCore.Components" />
        <activity android:name=".AndroidCore.Localisation" />
        <activity android:name=".AndroidCore.SnackbarAc" />
        <activity android:name=".AndroidCore.ToastAc" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".ServiceClass" />

        <receiver android:name=".BroadcastClass">
            <intent-filter>
                <action android:name="com.example.practicework.CustomIntent" />
            </intent-filter>
        </receiver>
    </application>

</manifest>